@model project5_voting.Models.Contact

@{
    ViewBag.Title = "Delete";
}


<main>
    <img src="~/images/delete.png" alt="Alternate Text" width="100" height="100" />

    <h3>Are you sure you want to delete this?</h3>

    <div>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-danger ml-1" />
                @Html.ActionLink("Back", "AdminContact", "Contacts", new { area = "" }, new { @class = "btn btn-secondary" })
            </div>
        }
    </div>
</main>
